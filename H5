<script>
/*
关闭弹窗		    没有返回值		app.close();
打开浏览器访问百度  没有返回值		app.open('http://www.baidu.com');
禁止截图   	    	没有返回值		app.closepng();
取网页GET源码 		返回文本		app.get('http://www.baidu.com');
复制内容到剪切板 	没有返回值		app.setcopy('复制的内容');
分享内容 			返回false true	app.share('分享的内容');
分享内容到QQ 		返回false true	app.share('分享的内容','com.tencent.mobileqq','com.tencent.mobileqq.activity.JumpActivity');
取已保存的文本		返回文本		app.toast(app.getstring('key'));
保存文本 			没有返回值 	 	app.setstring('key','内容');
创建进度对话框	 	没有返回值		app.newdialog('进度对话框内容');
改变进度对话框内容	没有返回值		app.setdialog('内容');
关闭进度对话框		没有返回值		app.closedialog();
结束软件 			没有返回值 		app.exit();
取app名称			返回空=不存在	app.appname('app包名');
弹窗提示			没有返回值		app.toast('提示内容');
取当前软件包名		返回文本		app.getpackage();
取设备唯一ID		返回文本		app.getuserid();
取MD5值				返回文本		app.getmd5('测试');
检测抓包			返回false true	app.isvpn();
打开顶部进度条     	没有返回值		app.topon();
关闭顶部进度条     	没有返回值		app.topoff();
设置弹窗高度      	没有返回值		app.setheight(500);

开发注意：不建议使用px 因为手机分辨率不一样  建议使用vw

*/
	function vpn(){
		if(app.isvpn()){
			app.toast('有vpn软件或wifi代理运行');
		}else{
			app.toast('一切正常');
		}
    }
	
    
	
	function get(url){
		app.newdialog('正在加载数据中...');
		/*app.get("http://www.baidu.com");*/
        var data=app.get(url);
		app.closedialog();
		/*var data=app.get(url,"User-Agent:Mozilla/4.0\nCookie:设置协议\n");
		var data=app.get(url,"User-Agent:Mozilla/4.0\nCookie:设置协议\n",3000);
		3000是超时时间3秒 默认是3秒
		data是返回的数据*/
		app.toast(data);
    }
	function post(url){
		app.newdialog('正在加载数据中...');			
        var data=app.post(url,"post=p&POST数据");
		app.closedialog();							
		/*var data=app.post(url,"post=p&POST数据","User-Agent:Mozilla/4.0\nCookie:设置协议\n");
		var data=app.post(url,"post=p&POST数据","User-Agent:Mozilla/4.0\nCookie:设置协议\n",3000);
		3000是超时时间3秒 默认是3秒
		data是返回的数据*/
		app.toast(data);
    }
	
    document.documentElement.style.webkitUserSelect='none';
    document.documentElement.style.webkitTouchCallout='none';
    </script>
</html>
